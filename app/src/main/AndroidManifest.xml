<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kat_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Data.ParseApp"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activities.ProjectDetailsActivity"></activity>
        <activity android:name=".Activities.ManageAccountActivity" />
        <activity android:name=".Activities.EditAccountActivity" />
        <activity android:name=".Activities.AddUpdateActivity" />
        <activity android:name=".Activities.SignupActivity" />
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.MainActivity" />
    </application>

</manifest>